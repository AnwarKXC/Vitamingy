ul(class=" cif justify-between text-center text-2xl maxlg:text-xl font-medium progress-timeline my-[50px] content-steps")
    li(class="w-full first active  relative") shipping
    li(class="w-full second active relative") payment
    li(class="w-full third relative") done
div(class="flex gap-[5%] maxlg:gap-4 container maxlg:flex-col capitalize " x-data="{data:'user',image:false,edit:false, newAddress:false}")
    side(class="lg:w-[340px] w-full     " )
        div(class="flex flex-col w-full px-[16px] border shadow")
            div( class="cif justify-between gap-2 py-3  relative border-b border-secondary")
                span(class="text-xl font-bold") SHOPPING CART
                a( href="./bag.html" class="font-medium underline") back to bag
            div(class="flex flex-col divide-y-[1px] w-full  border-b border-secondary")
                -for(let i=0;i<5;i++)
                    div(class="flex gap-2 py-2")
                        div(class="w-[70px]")
                            img( onerror="this.onerror=null;this.src='../assets/logo.jpg';" class="min-w-[70px] aspect-square" alt="" data-src="../assets/17-img-6.png")
                        div(class="grow")
                            h2(class="text-sm text-black capitalize font-bold") vitamine
                            div(class="text-sm  capitalize gap-1 flex items-center")
                                span(class="") 1
                                span(class="text-[10px] font-bold ") x
                                span(class="") 50.00 

            div(class="flex flex-col gap-2 py-1 mt-3 relative divide-y-[1px] font-medium" x-data="{coupon:false}")
                button(class="cent bg-secondary py-2 rounded-md text-white relative " @click="coupon=!coupon")
                    span(class="") add coupon
                form( class=" justify-between gap-2 py-2  relative hidden" x-show="coupon" :class="{'cif':coupon}")
                    input(class="rounded-md w-full px-4" placeholder="coupon..." type="text",id="coupon" name="coupon" required)
                    button(class="cent bg-secondary text-white text-xs py-2 rounded-md relative px-1" )
                        span(class="") submit
                div( class="cif justify-between gap-2 py-2  relative ")
                    span(class="") items 
                    span(class="") 5
                div(class="cif justify-between gap-2 py-2  relative " )
                    span(class="")  + shipping:
                    span(class="") 70 egp
                div( class="cif justify-between gap-2 py-2  relative ")
                    span(class="") Cash On Deliver Fees:
                    span(class="") 20 egp
                div( class="cif justify-between gap-2 py-2  relative bg-secondary/30")
                    span(class="") total order:
                    span(class="") 7020 egp



    main(class="grow" )

        div(class=" pb-6 pt-2 px-[16px] bg-white border shadow" x-show="data=='user'") 
            
            div(class="flex flex-col gap-2 py-2  relative "  )
                div(class="flex flex-col gap-2 py-1  relative font-medium" )
                    h3(class="font-bold text-lg ") Shipping Address:
                    div(class="cif flex-wrap gap-[2px]  text-lg maxlg:text-sm relative ")
                        span(class="") Al Beheira
                        span - 
                        span(class="") Abou Al Matamer 
                        span - 
                        span(class="") Street:
                        span(class="") 12312
                        span - 
                        span(class="") Building:
                        span(class="")  12313
                        span - 
                        span(class="") Floor Number:
                        span(class="")  12313
                        span - 
                        span(class="")  Flat Number:
                        span(class="")  12313
                        span - 
                        span(class="")  Details:
                        span(class="") 12313
                    div(class="cif flex-wrap gap-[2px]  text-lg maxlg:text-sm relative ")
                        span(class="") Shipping By :

                        span(class="text-primary")  vitamingy Shipping 

                    div(class="cif flex-wrap gap-[2px]  text-lg maxlg:text-sm relative ")

                        span(class="") The order will arrive in:
                        span(class="text-primary") 2 Days



       
       
        div(class="payment mt-4 pt-4 font-bold text-lg border-t-2 border-secondary ")
            p(class="") payment Method 
            p(class="") how would you like to pay ?  
        form(class=" " x-data="{option:'',check:''}")

            div(class="delivery-option w-full py-2 mt-8 px-[16px] rounded-md bg-white border")
                div(class="cif flex-wrap justify-between paymentCard paymentCard p-4 rounded-md mb-2 border w-full"  @click="check='Debit';option='Debit'" :class="{'payment-background':option=='Debit'}")


                    div(class="input")
                        label(class="radio inline-block relative pl-[30px] cursor-pointer align-middle")
                            input(type="radio", required="", x-bind:checked="check=='Debit'" name="payment_type", value="myFatoorahCard")
                            | Credit or Debit Card
                            span( class="payment-span  h-[20px] w-[20px] rounded-full border-[3px] border-secondary block absolute left-0 top-2 "  )
                        p(class="caption") Pay with Your Debit Or Credit Card  
                    div(class="flex gap-2")
                        img( onerror="this.onerror=null;this.src='../assets/logo.jpg';" class="h-[20px]"   alt="d", src="https://olbac.com/assets/website/images/footer/Visa_logo.jpg.png")
                        img( onerror="this.onerror=null;this.src='../assets/logo.jpg';" class="h-[20px]"   alt="d", src="https://olbac.com/assets/website/images/footer/Maestro_logo.jpg.png")
                
                
                
                
                
                div(class="cif flex-wrap justify-between paymentCard p-4 rounded-md mb-2 border w-full"  @click="check='MEZA';option='MEZA'" :class="{'payment-background':option=='MEZA'}")
                    div(class="input")
                        label(class="radio inline-block relative pl-[30px] cursor-pointer align-middle")
                            input(type="radio", required="", x-bind:checked="check=='MEZA'" name="payment_type", value="myFatoorahMeza")
                            | MEZA Card
                            span( class="payment-span  h-[20px] w-[20px] rounded-full border-[3px] border-secondary block absolute left-0 top-2 "  )
                        p(class="caption") Pay with Your MEZA  Card 
                    div(class="")
                        img( onerror="this.onerror=null;this.src='../assets/logo.jpg';" class="h-[30px]" , alt="d", src="https://olbac.com/assets/website/images/meza.jpg")
                
                
                
                
                div(class="cif flex-wrap justify-between paymentCard p-4 rounded-md mb-2 border w-full"  @click="check='Wallet';option='Wallet'" :class="{'payment-background':option=='Wallet'}")
                    div(class="input")
                        label(class="radio inline-block relative pl-[30px] cursor-pointer align-middle")
                            input(type="radio", required="", x-bind:checked="check=='Wallet'" name="payment_type", value="myFatoorahWallet")
                            | Wallet
                            span( class="payment-span  h-[20px] w-[20px] rounded-full border-[3px] border-secondary block absolute left-0 top-2 "  )
                        p(class="caption ")  Pay with Your Wallet (Vodafone - Etisalat - Orange - WE)  
                    div(class="")
                        div(class="flex gap-2 wallet")
                            img( onerror="this.onerror=null;this.src='../assets/logo.jpg';" class="h-[30px] aspect-square"   alt="d", src="https://olbac.com/assets/website/images/payment/v-cash.png")
                            img( onerror="this.onerror=null;this.src='../assets/logo.jpg';" class="h-[30px] aspect-square"   alt="d", src="https://olbac.com/assets/website/images/payment/o-cash.png")
                            img( onerror="this.onerror=null;this.src='../assets/logo.jpg';" class="h-[30px] aspect-square"   alt="d", src="https://olbac.com/assets/website/images/payment/w-cash.png")
                            img( onerror="this.onerror=null;this.src='../assets/logo.jpg';" class="h-[30px] aspect-square"   alt="d", src="https://olbac.com/assets/website/images/payment/e-cash.jpeg")
               
                div(class="cif flex-wrap justify-between paymentCard p-4 rounded-md mb-2 border w-full" @click="check='Fawry';option='Fawry'" :class="{'payment-background':option=='Fawry'}"  )
                    div(class="input")
                        label(class="radio inline-block relative pl-[30px] cursor-pointer align-middle")
                            input(type="radio", required="", x-bind:checked="check=='Fawry'" name="payment_type", value="fawry")
                            | Fawry Pay
                            span( class="payment-span  h-[20px] w-[20px] rounded-full border-[3px] border-secondary block absolute left-0 top-2 "  )
                        p(class="caption")  Pay with Fawry  
                    div(class="")
                        img( onerror="this.onerror=null;this.src='../assets/logo.jpg';" class="h-[30px] "   alt="d", src="https://olbac.com/assets/website/images/fawry.png")
               
               
               
               
               
               
                div(class="cif flex-wrap justify-between paymentCard p-4 rounded-md mb-2 border w-full " @click="check='cash';option='cash'" :class="{'payment-background':option=='cash'}")
                    div(class="input")
                        label(class="radio inline-block relative pl-[30px] cursor-pointer align-middle")
                            input(type="radio", required="", x-bind:checked="check=='cash'" name="payment_type", value="cash")
                            | Cash On Delivery
                            span( class="payment-span  h-[20px] w-[20px] rounded-full border-[3px] border-secondary block absolute left-0 top-2 "  )
                        p(class="caption", ) pay in cash
                    div(class="")
                        img( onerror="this.onerror=null;this.src='../assets/logo.jpg';" class="h-[30px] "  alt="d", src="https://olbac.com/assets/website/images/order/cash_en.png")

            div.cif.w-full.mt-8
                button(type="submit" class="py-2 px-6 text-center bg-secondary rounded-md text-white  w-full font-medium text-lg tracking-wider  uppercase") continue with pay








